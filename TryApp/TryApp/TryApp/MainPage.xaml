             mc:Ignorable="d"
             x:Class="TryApp.MainPage">

    <ContentPage.Content>
        <StackLayout Margin="5,30,5,5">
            <Label >
            </Label>

            <Button x:Name="goToPage1" Text="Go to Page 1" BackgroundColor="Yellow"></Button>

            <Button x:Name="goToPage2" Text="Go to Page 2" BackgroundColor="Red"></Button>

